<h1>ユーザー一覧</h1>

<table>
  <tr>
    <th>ID</th>
    <th>名前</th>
    <th>メール</th>
    <th></th>
  </tr>

  <% @users.each do |user| %>
    <tr>
      <td><%= user.id %></td>
      <td><%= user.name %></td>
      <td><%= user.email %></td>
      <td><%= link_to "詳細", admin_user_path(user) %></td>
    </tr>
  <% end %>
</table>